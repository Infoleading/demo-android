<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="edu.cque.jianxing.demointentfilter.DataCaseActivity" >

    <Button
        android:id="@+id/btn21"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="启动 uri=paper的Activity"
        android:onClick="onBtn21Click" />

    <Button
        android:id="@+id/btn22"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn21"
        android:layout_centerHorizontal="true"
        android:text="启动 uri=paper:local的Activity"
        android:onClick="onBtn22Click" />

    <Button
        android:id="@+id/btn23"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn22"
        android:layout_centerHorizontal="true"
        android:text="启动 uri=paper:local:8090的Activity"
        android:onClick="onBtn23Click" />

    <Button
        android:id="@+id/btn24"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn23"
        android:layout_centerHorizontal="true"
        android:text="启动 uri=paper:local:8090/test的Activity" 
        android:onClick="onBtn24Click"/>

    <Button
        android:id="@+id/btn25"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/btn24"
        android:layout_centerHorizontal="true"
        android:text="启动 uri=paper:local:8090/test type=txt/html 的Activity" 
        android:onClick="onBtn25Click"/>

</RelativeLayout>