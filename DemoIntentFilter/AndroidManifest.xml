<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.cque.jianxing.demointentfilter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SecondActivity"
            android:label="SecondActivity" >
            <intent-filter>
                <action android:name="edu.cque.jianxing.intent.action.MY_ACTION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataCaseActivity"
            android:label="@string/title_activity_data_case" >
        </activity>
        <activity
            android:name=".Data21Activity"
            android:label="@string/title_activity_data21" >
        </activity>
        <activity
            android:name=".Data22Activity"
            android:label="@string/title_activity_data22" >
        </activity>
        <activity
            android:name=".Data23Activity"
            android:label="@string/title_activity_data23" >
        </activity>
        <activity
            android:name=".Data24Activity"
            android:label="@string/title_activity_data24" >
        </activity>
        <activity
            android:name=".Data25Activity"
            android:label="@string/title_activity_data25" >
        </activity>
    </application>

</manifest>
