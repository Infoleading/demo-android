<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.cque.jianxing.demointentdatatype"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name=".DataOneActivity"
            android:label="@string/title_activity_data_one" >
            <intent-filter>
                <action android:name="xx" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:scheme="book" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataTwoActivity"
            android:label="@string/title_activity_data_two" >
            <intent-filter>
                <action android:name="xx" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="local.cn"
                    android:scheme="book" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataThreeActivity"
            android:label="@string/title_activity_data_three" >
            <intent-filter>
                <action android:name="xx" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="local.cn"
                    android:port="9090"
                    android:scheme="book" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataFourActivity"
            android:label="@string/title_activity_data_four" >
            <intent-filter>
                <action android:name="xx" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="local.cn"
                    android:path="/computer"
                    android:port="9090"
                    android:scheme="book" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DataFiveActivity"
            android:label="@string/title_activity_data_five" >
            <intent-filter>
                <action android:name="xx" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="local.cn"
                    android:mimeType="txt/html"
                    android:path="/computer"
                    android:port="9090"
                    android:scheme="book" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LuncherActivity"
            android:label="@string/title_activity_luncher" >
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>            
        </activity>
    </application>

</manifest>